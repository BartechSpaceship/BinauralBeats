<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.therealbinauralexample">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">
        <activity android:name=".First_LastPage.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Choices.SpiritBellController"
            android:parentActivityName=".First_LastPage.MainActivity"/>
        <activity android:name=".Choices.SleepBellController"
            android:parentActivityName=".First_LastPage.MainActivity"/>
        <activity android:name=".First_LastPage.LastPage"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".Choices.BrainBellController"
            android:parentActivityName=".First_LastPage.MainActivity"/>
        <activity android:name=".Choices.StudyBellController"
            android:parentActivityName=".First_LastPage.MainActivity"/>
        <activity android:name=".Choices.BodyBellController"
            android:parentActivityName=".First_LastPage.MainActivity"/>

    </application>

</manifest>